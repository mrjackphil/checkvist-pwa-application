{
	"defaultSeverity": "warning",
	"extends": [
		"tslint:recommended"
	],
	"linterOptions": {
		"exclude": [
			"node_modules/**"
		]
	},
	"rules": {
		"quotemark": [true, "double"],
		"arrow-parens": false,
		"trailing-comma": false,
		"indent": [true, "spaces", 2],
		"interface-name": false,
		"ordered-imports": false,
		"object-literal-sort-keys": false,
		"no-consecutive-blank-lines": false
	}
}